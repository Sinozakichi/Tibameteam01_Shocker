    <script src="~/axios/axios.min.js"></script>
    <script src="~/vue/vue.global.prod.js"></script>
    <script>
        var baseAddress = "https://localhosthttps://localhost:7259/CustomerQuestion/";
        var VueApp = {
            data(){
                return{
                        products:[],
                    
                
                };
            },
            mounted:function(){
             _this =this;
                _this.getProductData();
            },
            methods:{
                getProductData:function(){
                    _this = this;
                    axios.post(`${window.location.origin}/CustomerQuestion/GetProductlistApi`)
                        .then(response => { _this.products = response.data.data, alert(response.data.errorMessage) })
                }
            }
        };
        var app = Vue.createApp(VueApp).mount("#app");
    </script>
}